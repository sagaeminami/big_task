<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#gnavi">
        <span class="sr-only"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">メニュー</a>
    </div>
    <div id="gnavi" class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
        <li><a href="">ツイート一覧📖</a></li>
        <li><%= link_to 'つぶやく🖋', new_clone_path %></li>
        <li><a href="">ログアウト🚪</a></li>
      </ul>
    </div>
  </div>
</nav>
<div class="container">
  <article class-"tweet row"> 
      <% @clones.each do |clone| %>
        <div class-"blog-panel col-xs-12 col-md-4 col-md-offset-4">
          <h2><%= clone.contents %></h2>
          <p><%= clone.created_at %></p>
          <%= link_to 'Show', clone_path(clone.id)  %> |
          <%= link_to 'Edit', edit_clone_path(clone.id)%> |
          <%= link_to 'Destroy', clone_path(clone.id),method: :delete, data: { confirm: 'Are you sure?' }%>
        </div>
      <% end %>
  </article>
</div>


